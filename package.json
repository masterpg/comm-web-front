    "postcss-mixins": "^6.2.0",
    "postcss-nesting": "^6.0.0",
    "prettier": "^1.14.2",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.3",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  }
