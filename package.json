  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterpg/lived-web-front.git"
  },
  "scripts": {
    "build:styles": "node_modules/.bin/cpx \"src/styles/**/*.{pcss,js}\" lib/styles",
    "watch:styles": "node_modules/.bin/cpx \"src/styles/**/*.{pcss,js}\" lib/styles --watch",
    "build:ts": "node_modules/.bin/tslint -p tslint.json & node_modules/.bin/tsc --project tsconfig.json --declaration",
    "watch:ts": "node_modules/.bin/tslint -p tslint.json & node_modules/.bin/tsc --project tsconfig.json --declaration --watch",
    "build": "npm run build:styles & npm run build:ts",
    "watch": "npm run watch:styles & npm run watch:ts",
    "dev": "npm run watch & node_modules/.bin/webpack-dev-server --config webpack.dev.config.js",
    "test": "node_modules/.bin/webpack --config webpack.dev.config.js && node_modules/.bin/mocha-chrome .dist/test.html"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.2",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-resizable-behavior": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/polymer": "~3.0.5",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "gsap": "^2.0.2",
    "lodash": "^4.17.10",
    "qs": "github:ljharb/qs"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "cpx": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.1.0",
    "postcss-cli": "^6.0.0",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.0",
