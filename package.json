{
  "name": "comm-web-front",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "author": "masterpg",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/masterpg/comm-web-front.git"
  },
  "scripts": {
    "build:styles": "cpx \"src/styles/placeholder/*.css\" lib/styles/placeholder & cpx \"src/styles/variables/*.css\" lib/styles/variables & cpx \"src/styles/polymer/**/*.js\" lib/styles/polymer & postcss --config postcss.config.js src/styles/classes/**/*.css --base src --dir lib",
    "watch:styles": "cpx \"src/styles/placeholder/*.css\" lib/styles/placeholder --watch & cpx \"src/styles/variables/*.css\" lib/styles/variables --watch & cpx \"src/styles/polymer/**/*.js\" lib/styles/polymer --watch & postcss --config postcss.config.js src/styles/classes/**/*.css --base src --dir lib --watch",
    "build:ts": "eslint ./ --ext .js,.ts & tsc --project tsconfig.json --declaration",
    "watch:ts": "esw ./ --ext .js,.ts --watch & tsc --project tsconfig.json --declaration --watch",
    "build": "npm run build:styles & npm run build:ts",
    "watch": "npm run watch:styles & npm run watch:ts",
    "dev": "npm run watch & webpack-dev-server --config demo/webpack.config.js",
    "test": "webpack --config demo/webpack.config.js && mocha-chrome demo/dist/test.html",
    "lint": "./scripts/lint.sh"
  },
  "dependencies": {
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/polymer": "^3.2.0",
    "animejs": "^3.0.1",
    "lit-element": "^2.1.0",
    "lodash": "^4.17.11",
    "qs": "github:ljharb/qs"
  },
  "devDependencies": {
    "@polymer/app-layout": "^3.0.2",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-radio-group": "^3.0.1",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "autoprefixer": "^9.5.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.2",
    "core-js": "3",
    "cpx": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-watch": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "mocha-chrome": "^2.0.0",
    "postcss-cli": "^6.1.2",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nesting": "^7.0.0",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
